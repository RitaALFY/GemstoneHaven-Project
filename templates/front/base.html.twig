<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="shortcut icon" type="image/jpg" href="{{ asset('images/icons/gemstone_haven.jpg') }}">
        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.1/dist/umd/popper.min.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha2/js/bootstrap.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
                integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
                crossorigin="anonymous"></script>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block leogout_seo %}
        {% endblock leogout_seo %}

        {% block stylesheets %}
            {{ encore_entry_link_tags('front_styles') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('front_scripts') }}
        {% endblock %}
    </head>
    <body>
    {% block header %}
        {% include 'front/common/header.html.twig' %}
    {% endblock %}

    <div class="container">
        {% block body %}
        {% endblock %}
    </div>

    {% block metadata %}
    {% endblock %}

    {% block footer %}
        {% include 'front/common/footer.html.twig' %}
    {% endblock %}
    </body>

</html>
